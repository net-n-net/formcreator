                </TCEforms>
                <type>array</type>
                <el>
				<label>Checkbox</label>
                    <settings.override.form>
                        <TCEforms>
                            <label>LLL:EXT:formcreator/Resources/Private/Language/locallang_flexform.xlf:showTitle</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_formcreator_domain_model_form</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>1</minitems>
								<show_thumbs>1</show_thumbs>
							</config> 
                        </TCEforms>
                    </settings.override.form>
					<settings.override.js-required>
                        <TCEforms>
                            <label>LLL:EXT:formcreator/Resources/Private/Language/locallang_flexform.xlf:js-required</label>
							<config>
								<type>check</type>
							</config>
                        </TCEforms>
                    </settings.override.js-required>
					<settings.override.sender-name>	
						<TCEforms>
							<label>LLL:EXT:formcreator/Resources/Private/Language/locallang_flexform.xlf:sender-name</label>
							<config>
							  <type>input</type>
							  <size>20</size>
							  <max>100</max>
							  <eval>trim</eval>
							</config>
						</TCEforms>							
                    </settings.override.sender-name>					
                    <settings.override.sender-email>
						<TCEforms>					
							<label>LLL:EXT:formcreator/Resources/Private/Language/locallang_flexform.xlf:sender-email</label>
							<config>
							  <type>input</type>
							  <size>20</size>
							  <max>100</max>
							  <eval>trim</eval>
							</config>
						</TCEforms>						
                    </settings.override.sender-email>					
                    <settings.override.name>	
						<TCEforms>
							<label>LLL:EXT:formcreator/Resources/Private/Language/locallang_flexform.xlf:name</label>
							<config>
							  <type>input</type>
							  <size>20</size>
							  <max>100</max>
							  <eval>trim,required</eval>
							</config>
						</TCEforms>							
                    </settings.override.name>					
                    <settings.override.email>
						<TCEforms>					
							<label>LLL:EXT:formcreator/Resources/Private/Language/locallang_flexform.xlf:email</label>
							<config>
