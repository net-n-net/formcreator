                    </settings.override.email>
                    <settings.override.subject>
						<TCEforms>					
							<label>LLL:EXT:formcreator/Resources/Private/Language/locallang_flexform.xlf:subject</label>
							<config>
							  <type>input</type>
							  <size>20</size>
							  <max>100</max>
							  <eval>trim,required</eval>
							</config>
						</TCEforms>						
                    </settings.override.subject>					
                   <settings.override.redirect>
						<TCEforms>					
							<label>LLL:EXT:formcreator/Resources/Private/Language/locallang_flexform.xlf:redirect</label>
							<config>
							  <type>group</type>
							  <internal_type>db</internal_type>
